  journal = {Methods in Enzymology},
  keywords = {Adoptive immunotherapy,Flow cytometry,Gamma/delta T cells,Humans,Immunotherapy; Adoptive,Neoplasms,Phosphoantigen,Receptors; Antigen; T-Cell; gamma-delta,T-Lymphocytes,Vitamin C,Vγ9Vδ2 T cells,Zoledronate},
